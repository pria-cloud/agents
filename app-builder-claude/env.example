# Claude Code SDK App-Builder Agent Environment Configuration

# Required - Anthropic API Key for Claude Code SDK
ANTHROPIC_API_KEY=your_anthropic_api_key_here

# Required - A2A Router Configuration
A2A_ROUTER_URL=http://localhost:9999
A2A_API_KEY=your_a2a_api_key_here

# Required - Supabase Configuration
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_ANON_KEY=your_supabase_anon_key_here

# Optional - Deployment Configuration
APP_BUILDER_CLAUDE_PUBLIC_URL=https://your-deployment-url.vercel.app
PORT=4002

# Optional - Application Settings
LOG_LEVEL=info
JSON_LIMIT=25mb
NODE_ENV=development

# Optional - OpenTelemetry Configuration
OTEL_EXPORTER_OTLP_ENDPOINT=http://localhost:4318/v1/traces

# Optional - GitHub Integration (if needed)
GITHUB_TOKEN=your_github_token_here
TARGET_REPO=your-org/your-repo 